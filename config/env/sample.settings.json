{
    "mongo": "mongodb://MONGO_CONNECTION_STRING",

    "agenda": {
        "connection": "mongodb://MONGO_CONNECTION_STRING",
        "jobs": ["LIST_OF_JOBS_TO_RUN"]
    },

    "gitlab": {
        "key": "GITLAB_APPLICATION_ID",
        "secret": "GITLAB_APPLICATION_SECRET",    

        "url": "http://GITLAB_BASE_URL/",
        "api": "http://GITLAB_BASE_URL/api/v3/",
        "grant_url": "http://GITLAB_BASE_URL/oauth/token"
    },

    "active_directory": {
        "url": "ldap://LDAP_SERVER_URL:LDAP_PORT/",
        "baseDN": "BASE_DN",
        "domain": "LDAP_DOMAIN"
    },

    "log_level": "trace"
}